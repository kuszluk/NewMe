    {# DIV PRZEGLĄD LIST #}
    <div class="col-sm-6">

        {# BOX PRZEGLĄD LIST #}
        <div class="box box-primary box-500">

            {# BOX HEADER #}
            <div class="box-header with-border">
                <h3 class="box-title">Przeglądaj swoje listy</h3>
            </div>

            {# BOX BODY #}
            <div class="box-body mh-415">

                {#  BRAK LIST  #}
                {%  if lists|length == 0 %}

                    <div class="empty-msg">
                        <h3>Brak list!</h3>
                        <p>Kliknij zielony przysk poniżej, żeby stworzyć swoją pierwszą liste.</p>
                        <p> <a href="/add"><button type="button" class="btn btn-success"><i class="fa fa-plus-circle"></i></button></a> </p>
                    </div>

                {% endif %}

                {# WYŚWIETLANIE LIST #}
                {% for list in lists %}

                    <div class="info-box bg-{{ lists.color }}">
                        <span class="info-box-icon"><i class="fa fa-{{ lists.icon }}"></i></span>
                        <div class="info-box-content">
                            <span class="info-box-text">{{ lists.title }}</span>
                            <div class="progress">
                                <div class="progress-bar" style="width: {{ lists.percent }}%"></div>
                            </div>
                            <span class="progress-description">
                                {{ lists.percent }}% ukończone | Edytuj
                            </span>
                        </div>
                    </div>

                {% endfor %}

