{% block lista_list %}

    <div class="col-sm-6">
        <div class="box box-primary box-500">

            <div class="box-header with-border">
                <h3 class="box-title">Przeglądaj swoje listy</h3>
            </div><!-- /.box-header -->
            <div class="box-body mh-415">

                <div class="info-box bg-red">
                    <span class="info-box-icon"><i class="fa fa-comments-o"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Życie towarzyskie</span>
                        <div class="progress">
                            <div class="progress-bar" style="width: 20%"></div>
                        </div>
                        <span class="progress-description">
                                20% ukończone | Edytuj
                            </span>
                    </div>
                </div>

                <div class="info-box bg-aqua">
                    <span class="info-box-icon"><i class="fa fa-bookmark-o"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Ksiązki i filmy</span>
                        <div class="progress">
                            <div class="progress-bar" style="width: 50%"></div>
                        </div>
                        <span class="progress-description">
                                50% ukończone | Edytuj
                            </span>
                    </div><!-- /.info-box-content -->
                </div>

                <div class="info-box bg-green">
                    <span class="info-box-icon"><i class="fa fa-thumbs-o-up"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Prowadzenie bloga</span>
                        <div class="progress">
                            <div class="progress-bar" style="width: 30%"></div>
                        </div>
                        <span class="progress-description">
                                30% ukończone | Edytuj
                            </span>
                    </div><!-- /.info-box-content -->
                </div>

                <div class="info-box bg-yellow">
                    <span class="info-box-icon"><i class="fa fa-calendar"></i></span>
                    <div class="info-box-content">
                        <span class="info-box-text">Imprezy</span>
                        <div class="progress">
                            <div class="progress-bar" style="width: 10%"></div>
                        </div>
                        <span class="progress-description">
                                10% ukończone | Edytuj
                            </span>
                    </div><!-- /.info-box-content -->
                </div>

            </div><!-- /.box-body -->
            <div class="box-footer">
                <div class="btn-group pull-right">
                    <button type="button" class="btn btn-default"><i class="fa fa-print"></i></button>
                    <button type="button" class="btn btn-default"><i class="fa fa-file-pdf-o"></i></button>
                    <button type="button" class="btn btn-success"><i class="fa fa-plus-circle"></i></button>
                </div>
            </div>
        </div>
    </div>

{% endblock %}